    {
      "cell_type": "code",
      "source": [
        "import json\n",
        "\n",
        "def add_genre_name(j):\n",
        "    genres = []\n",
        "    ar = json.loads(j)\n",
        "    for a in ar:\n",
        "        genres.append(a.get(\"name\"))\n",
        "    return \" \".join(sorted(genres))\n",
        "\n",
        "movies['genres_name'] = movies.apply(lambda x: add_genre_name(x.genres), axis=1)"
      ],
      "metadata": {
        "id": "kpTKqewYNucD"
      },
      "execution_count": 13,
      "outputs": []
