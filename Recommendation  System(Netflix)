        "def reco_top_scored_one(n,genre=None):\n",
        "  if genre is None:\n",
        "    return popularity[\"original_title\"].head(n)\n",
        "  else:\n",
        "    return popularity[popularity['genres_name'].str.contains(genre)]['original_title'].head(n)"
      ],
